Cell[BoxData[{
 RowBox[{
  RowBox[{"variables", "=", 
   RowBox[{"<|", 
    RowBox[{
     RowBox[{"\"\<indVars\>\"", "->", 
      RowBox[{"{", 
       RowBox[{"n", ",", "m"}], "}"}]}], ",", 
     RowBox[{"\"\<depVars\>\"", "->", 
      RowBox[{"{", 
       RowBox[{"u", ",", "v"}], "}"}]}], ",", 
     RowBox[{"\"\<pars\>\"", "\[Rule]", 
      RowBox[{"{", "a", "}"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<scheme\>\"", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{
           RowBox[{"n", "+", "1"}], ",", "m"}], "]"}], "-", 
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "m"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"v", "[", 
          RowBox[{
           RowBox[{"n", "+", "1"}], ",", "m"}], "]"}], "-", 
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "m"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"v", "[", 
          RowBox[{"n", ",", 
           RowBox[{"m", "+", "1"}]}], "]"}], "-", 
         RowBox[{"v", "[", 
          RowBox[{"n", ",", "m"}], "]"}]}]}], "}"}]}]}], "|>"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"expression", "=", 
   RowBox[{
    RowBox[{"u", "[", 
     RowBox[{
      RowBox[{"n", "+", "1"}], ",", "m"}], "]"}], "-", 
    RowBox[{"2", " ", 
     RowBox[{"u", "[", 
      RowBox[{
       RowBox[{"n", "-", "1"}], ",", 
       RowBox[{"m", "+", "2"}]}], "]"}]}], "-", 
    RowBox[{"v", "[", 
     RowBox[{
      RowBox[{"n", "-", "1"}], ",", 
      RowBox[{"m", "-", "2"}]}], "]"}], "+", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"a", "+", 
        RowBox[{"u", "[", 
         RowBox[{"n", ",", 
          RowBox[{"m", "+", "4"}]}], "]"}], "-", 
        RowBox[{"v", "[", 
         RowBox[{
          RowBox[{"n", "-", "1"}], ",", 
          RowBox[{"m", "-", "2"}]}], "]"}]}], ")"}], "^", "2"}], " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"u", "[", 
         RowBox[{
          RowBox[{"n", "+", "3"}], ",", 
          RowBox[{"m", "-", "4"}]}], "]"}], "-", 
        RowBox[{"v", "[", 
         RowBox[{
          RowBox[{"n", "-", "1"}], ",", 
          RowBox[{"m", "+", "2"}]}], "]"}]}], ")"}], "^", "2"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ReduceModSchemeOperator", "[", "variables", "]"}], "[", 
