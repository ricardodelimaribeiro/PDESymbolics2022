Cell["\<\
Instead of using the standard forward difference, one can also specify \
another \"timedifference\" in the variables, that TimeDifferenceOperator will \
then use to build the appropriate time-difference:\
\>", "ExampleText",
 CellChangeTimes->{{3.861086454613585*^9, 3.861086487215098*^9}, {
  3.861091354600501*^9, 3.861091388908156*^9}},
 CellID->641812077,ExpressionUUID->"2be5127d-fe91-44ae-b734-6931e0a347b0"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"variables", "=", 
   RowBox[{"<|", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\"\<depVars\>\"", "->", 
       RowBox[{"{", "u", "}"}]}], ",", 
      RowBox[{"\"\<indVars\>\"", "->", 
       RowBox[{"{", 
        RowBox[{"n", ",", "t"}], "}"}]}], ",", 
      RowBox[{"\"\<timedifference\>\"", "->", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"1", "/", "2"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"#", "/.", " ", 
              RowBox[{"t", "->", 
               RowBox[{"t", "+", "1"}]}]}], ")"}], "-", 
            RowBox[{"(", 
             RowBox[{"#", "/.", " ", 
              RowBox[{"t", "->", 
               RowBox[{"t", "-", "1"}]}]}], ")"}]}], ")"}]}], "&"}], 
        ")"}]}]}], "}"}], "|>"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TimeDifferenceOperator", "[", "variables", "]"}], "[", 
  RowBox[{"u", "[", 
