(* Wolfram Language Test file *)
	test = "Tests/DVarDOperatorChild.mt";
		Print["   DVarDOperator"];

label = "$Failed"
variables = Association[
	"expression" -> $Failed, 
	"result" -> $Failed
	];
Get[ test ]

label = "Mechanics"
variables = Association[{
	"depVars" -> {u},
	"indVars" -> {n},
	"expression" -> (u[n+1]-u[n])^2 + u[n]^2,
	"result" -> {2*u[n] + 2*(-u[-1 + n] + u[n]) - 2*(-u[n] + u[1 + n])}
}];
Get[ test ]

label = "Mechanics with DiefferenceDelta"
variables = Association[{
	"depVars" -> {u},
	"indVars" -> {n},
	"expression" -> DifferenceDelta[u[n],{n,1}]^2 + u[n]^2,
	"result" -> {2*u[n] + 2*(-u[-1 + n] + u[n]) - 2*(-u[n] + u[1 + n])}
}];
Get[ test ]

label = "Second derivative"
variables = Association[{
	"depVars" -> {u},
	"indVars" -> {n},
	"expression" -> u[n]*DifferenceDelta[u[n],{n,1}] + DifferenceDelta[u[n],{n,2}]^2 + DifferenceDelta[u[n],{n,3}]^2,
	"result" -> {u[-1 + n] - 2*u[n] + 2*(-u[-3 + n] + 3*u[-2 + n] - 3*u[-1 + n] + u[n]) + 2*(u[-2 + n] - 2*u[-1 + n] + u[n]) + u[1 + n] - 6*(-u[-2 + n] + 3*u[-1 + n] - 3*u[n] + u[1 + n]) - 4*(u[-1 + n] - 2*u[n] + u[1 + n]) + 6*(-u[-1 + n] + 3*u[n] - 3*u[1 + n] + u[2 + n]) + 2*(u[n] - 2*u[1 + n] + u[2 + n]) - 2*(-u[n] + 3*u[1 + n] - 3*u[2 + n] + u[3 + n])}
